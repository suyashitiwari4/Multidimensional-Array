#include<iostream>
using namespace std;
int main(){
    int matrix[3][3];
    for(int i=0;i<3;i++){
        for(int j=0;j<3;j++){
            cout<<"Enter element-("<<i<<j<<"): ";
            cin>>matrix[i][j];
        }
    }
    for(int k=0;k<3;k++){
        for(int l=0;l<3;l++){
            cout<<matrix[k][l];
            cout<<" ";
        }
        cout<<endl;
    }
    return 0;

}
/*Output
Enter element-(00): 1
Enter element-(01): 2
Enter element-(02): 3
Enter element-(10): 4
Enter element-(11): 5
Enter element-(12): 6
Enter element-(20): 7
Enter element-(21): 8
Enter element-(22): 9
1 2 3 
4 5 6 
7 8 9 


=== Code Execution Successful ===*/
